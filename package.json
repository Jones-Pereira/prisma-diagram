{
  "name": "prisma-editor",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "web": "pnpm --filter @prisma-editor/web",
    "dmmf": "pnpm --filter @prisma-editor/prisma-dmmf-extended"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest",
    "@types/node": "^18.14.1",
    "prettier-plugin-tailwindcss": "^0.2.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.27.1"
}
