datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id     Int   @id @default(autoincrement())
  post   Post? @relation(name: "UserToPost", fields: [postId], references: [id])
  postId Int?  @unique
}

model Post {
  id   Int   @id @default(autoincrement())
  testingg User? @relation(name: "UserToPost")
}

enum Role {
  USER
  ADMIN
}
